  <DataSets>
    <DataSet Name="IncompleteGradYear">
      <Query>
        <DataSourceName>MBADatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SchoolName">
            <Value>=Parameters!SchoolName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Sch.SchoolName, Stu.StudentSSNum, Stu.StudentName, Stu.ExpectedGrad,
(CASE WHEN 
		Min(CASE WHEN cc.CourseCode IS NOT NULL THEN 1 ELSE 0 END ) = 1
THEN 'Complete' ELSE 'Incomplete' END ) AS 'Foundation Courses Status'

FROM School Sch
INNER JOIN Student Stu ON Sch.SchoolName = Stu.SchoolName
INNER JOIN Course C ON Sch.CurriculumCode = C.CurriculumCode
LEFT JOIN CourseCompleted CC on stu.StudentSSNum = cc.StudentSSNum and C.CourseCode = CC.CourseCode
WHERE C.TypeID = 1 AND YEAR(Stu.ExpectedGrad) &lt; YEAR(DATEADD(year, 2 , GETDATE())) AND Sch.SchoolName=@SchoolName
GROUP BY Stu.StudentSSNum, Sch.SchoolName, Stu.StudentName, stu.ExpectedGrad
HAVING Min(CASE WHEN cc.CourseCode IS NOT NULL THEN 1 ELSE 0 END ) = 0
ORDER BY [Foundation Courses Status]</CommandText>
      </Query>
      <Fields>
        <Field Name="SchoolName">
          <DataField>SchoolName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudentSSNum">
          <DataField>StudentSSNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudentName">
          <DataField>StudentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ExpectedGrad">
          <DataField>ExpectedGrad</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Foundation_Courses_Status">
          <DataField>Foundation Courses Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
